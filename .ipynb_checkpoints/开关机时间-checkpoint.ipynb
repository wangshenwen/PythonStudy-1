    "import os, datetime\n",
    "def main():\n",
    "    print(\"电脑开机时间:\")\n",
    "    dt=datetime.datetime.fromtimestamp(psutil.boot_time())\n",
    "    print(dt.strftime(\"%Y-%m-%d,%H:%M:%S\"))\n",
    "if __name__==\"__main__\":\n",
    "    main()\n"
